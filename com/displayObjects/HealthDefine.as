package com.displayObjects{	import flash.display.MovieClip;	import flash.utils.Timer;	import flash.events.TimerEvent;	import com.displayObjects.activeObj;	public class HealthDefine extends MovieClip {		private var healthTimer:Timer;		public function HealthDefine():void {			healthTimer=new Timer(100);			healthTimer.addEventListener(TimerEvent.TIMER, defineHealth, false, 0, true);			healthTimer.start();		}		private function defineHealth(e:TimerEvent):void {			if (parent!=null && parent is activeObj) {			var health:int=int(this.name.slice(1,this.name.length));			activeObj(parent).health=health;			activeObj(parent).healthTot=health;			healthTimer.stop();			healthTimer.removeEventListener(TimerEvent.TIMER, defineHealth, false);				parent.removeChild(this);			}		}	}}